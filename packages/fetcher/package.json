{
    "name": "@wasdadeel/fetcher",
    "description": "Tiny fetch wrapper with middleware, baseUrl, hooks (beforeFetch/afterFetch), redirect helper, and easy get/post/put/patch/delete helpers",
    "keywords": [
        "fetch",
        "http",
        "typescript",
        "middleware",
        "abortcontroller",
        "timeout",
        "baseUrl",
        "client",
        "request",
        "response",
        "node18",
        "browser",
        "isomorphic",
        "utilities",
        "helpers"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bandrw/wasdadeel.git",
        "directory": "packages/fetcher"
    },
    "author": "Andrei Balashov <bandrwx@gmail.com>",
    "version": "1.0.25",
    "type": "module",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/esm/index.d.ts",
            "import": "./dist/esm/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "sideEffects": false,
    "scripts": {
        "build:esm": "tsc -p tsconfig.json",
        "build": "rm -rf dist && yarn build:esm",
        "typecheck": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
        "lint:check": "exit 0",
        "lint:fix": "exit 1",
        "release": "yarn publish --new-version $(node -p \"require('./package.json').version\") --no-git-tag-version --no-commit-hooks",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "typescript": "^5"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT"
}
