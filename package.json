{
    "name": "wasdadeel",
    "version": "1.0.23",
    "workspaces": [
        "packages/*"
    ],
    "private": true,
    "engines": {
        "node": ">=18.0.0",
        "yarn": ">=1.22.19"
    },
    "scripts": {
        "release": "yarn sync-versions && node scripts/workspaces-run.js release",
        "build": "node scripts/workspaces-run.js build",
        "typecheck": "yarn workspaces run typecheck",
        "lint:check": "yarn workspaces run lint:check",
        "lint:fix": "yarn workspaces run lint:fix",
        "sync-versions": "node scripts/sync-versions.js"
    },
    "repository": "https://github.com/bandrw/wasdadeel",
    "author": "Andrei Balashov <bandrwx@gmail.com>",
    "packageManager": "yarn@1.22.19",
    "license": "MIT"
}
